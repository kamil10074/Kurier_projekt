@model IEnumerable<Kurier_projekt.Models.Kurierzy>

@{
    ViewBag.Title = "Index";
}

<h2>Pracownicy</h2>

<p>
    @Html.ActionLink("Dodaj nowego pracownika", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Imie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nazwisko)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data_urodzenia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data_zatrudnienia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adres_kurier)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_Pojazdu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Numer_telefonu)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Imie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nazwisko)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Data_urodzenia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Data_zatrudnienia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adres_kurier)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Pojazdu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numer_telefonu)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Kurier }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Kurier }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Kurier })
        </td>
    </tr>
}

</table>
